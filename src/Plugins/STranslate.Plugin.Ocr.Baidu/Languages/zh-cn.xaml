<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_ApiKey">API Key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_SecretKey">Secret Key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Action">版本选择</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Action_Description">「Accurate」为高精度版，「General」为标准版，「Basic」表明不带位置信息</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Official">官方网站</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Official_Description">点击下面连接跳转官方网站进行注册使用</sys:String>

</ResourceDictionary>