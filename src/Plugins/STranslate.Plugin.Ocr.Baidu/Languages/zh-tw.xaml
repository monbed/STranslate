<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_ApiKey">API Key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_SecretKey">Secret Key</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Action">版本選擇</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Action_Description">「Accurate」為高精度版，「General」為標準版，「Basic」表明不帶位置資訊。</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Official">官方網站</sys:String>
    <sys:String x:Key="STranslate_Plugin_Ocr_Baidu_Official_Description">點擊下面連結跳轉官方網站進行註冊使用。</sys:String>

</ResourceDictionary>